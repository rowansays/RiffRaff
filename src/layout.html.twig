<!DOCTYPE html>
<!--HI FROM ROWAN'S TEMPLATE :) -->
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{% block title %}{{ project.name }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {{ renderBaseUrlHeader() }}

    {% block stylesheets %}
        <link rel="stylesheet" href="style.css">
    {% endblock %}
    {% block javascripts %}
        <script src="https://cdn.jsdelivr.net/npm/fuse.js@3.4.6"></script>
        <script src="js/search.js"></script>
        <script defer src="js/searchIndex.js"></script>
    {% endblock %}
</head>
<body id="top">
  <header id="Nameplate">
    <div>
      <div class="Heading">
        <h1 id="SiteName" class="SiteName">
          <a href="{{ path('/') }}">Rowan says<span class="Bling">â€¦</span></a>
        </h1>
        <p id="Tagline" class="Tagline">{{ project.name }}</p>
      </div>
    </div>
  </header>
  <section>
    <input class="phpdocumentor-header__menu-button" type="checkbox" id="menu-button" name="menu-button">
    <label class="phpdocumentor-header__menu-icon" for="menu-button">
      <span class="fas fa-bars"></span>
    </label>

    <!-- Searchform Start -->
    <label>
        <span class="visually-hidden">Search for</span>
        <svg class="phpdocumentor-search__icon" width="21" height="20" viewBox="0 0 21 20" fill="none" xmlns="http://www.w3.org/2000/svg">
            <circle cx="7.5" cy="7.5" r="6.5" stroke="currentColor" stroke-width="2"/>
            <line x1="12.4892" y1="12.2727" x2="19.1559" y2="18.9393" stroke="currentColor" stroke-width="3"/>
        </svg>
        <input type="search" class="phpdocumentor-field phpdocumentor-search__field" placeholder="Loading .." disabled />
    </label>
    <!-- Searchform End -->

    {% include 'components/topnav.html.twig' %}
  </section>

  {% include 'components/sidebar.html.twig' %}
  <main id="Content">
    {% block content %}{% endblock %}
    {% include 'components/search-results.html.twig' %}
  </main>
</body>
</html>
