{
  "name": "@rowansays/riffraff",
  "version": "0.0.1",
  "description": "Custom template for phpDocumentor",
  "main": "index.js",
  "scripts": {
    "dist": "webpack --config ./webpack/dist.js",
    "make": "webpack serve --config ./webpack/make.js",
    "makeDocs": "phpdocWpHelpers && npm run make",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Rowan Weathers (https://rowanays.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime-corejs3": "^7.12.5",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "mini-css-extract-plugin": "^1.3.5",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.0.0"
  },
  "dependencies": {
    "babel-plugin-prismjs": "^2.0.1",
    "prismjs": "^1.23.0"
  },
  "babel": {
    "plugins": [
      ["prismjs", {
          "languages": ["php"],
          "plugins": ["line-numbers"],
          "css": true
      }]
    ]
  },
  "browserslist": [
    "last 2 versions",
    "Safari >= 10",
    "iOS >= 10",
    "not ie <= 10",
    "> 1%"
  ]
}
